page {
	height: 100vh;
	background-color: #fff;
}
.tab-text.active {
	color: rgba(53, 116, 250, 1);
}
.model-box {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	/* height: 376upx; */
	padding: 0 20rpx;
	-webkit-flex-wrap: wrap;
	        flex-wrap: wrap;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
	-webkit-box-shadow: 0px 10px 10px #eee;
	        box-shadow: 0px 10px 10px #eee;
	background-color: #fff;
	z-index: 1;
}
.model-box-text {
	/* width:98px; */
	/* height:24px; */
	font-size: 24rpx;
	font-family: SourceHanSansCN-Regular, SourceHanSansCN;
	font-weight: 400;
	color: rgba(51, 51, 51, 1);
	line-height: 36rpx;
	margin-top: 20rpx;
}
.model-item {
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
	padding: 24rpx 0;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	        flex-direction: column;
	-webkit-box-flex: 0;
	-webkit-flex: 0 0 25%;
	        flex: 0 0 25%;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
}
/* 底部弹出层 */
.bottom-title {
	text-align: left;
	padding: 15rpx;
	font-size: 26rpx;
	font-family: SourceHanSansCN-Regular, SourceHanSansCN;
	font-weight: 400;
	color: rgba(51, 51, 51, 1);
	letter-spacing: 2rpx;
}
.bottom-content {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	        flex-direction: row;
	-webkit-flex-wrap: wrap;
	        flex-wrap: wrap;
	padding: 0 20rpx;
}
.bottom-content-box {
	-webkit-box-flex: 0;
	-webkit-flex: 0 0 25%;
	        flex: 0 0 25%;
	padding: 40rpx 0;
}
.bottom-content-text {
	font-size: 24rpx;
	font-family: SourceHanSansCN-Regular, SourceHanSansCN;
	font-weight: 400;
	color: rgba(51, 51, 51, 1);
}
.list {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	        flex-direction: column;
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	        flex: 1;
	overflow-y: scroll;
	-webkit-overflow-scrolling: touch;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
	/* padding-left: 60upx; */
	/* height: 60upx; */		
	width: 90%;
	margin: 0 auto;
	white-space: nowrap;
	text-overflow: ellipsis;
	overflow: hidden;
}
.list-text {
	font-size: 24rpx;
	font-family: SourceHanSansCN-Regular, SourceHanSansCN;
	font-weight: 400;
	color: rgba(102, 102, 102, 1);
	background: rgba(255,255,255,1);
	-webkit-box-shadow: 0rpx 1rpx 4rpx 0rpx rgba(187,187,187,0.5);
	        box-shadow: 0rpx 1rpx 4rpx 0rpx rgba(187,187,187,0.5);
	border-radius: 6rpx;
	border: 1rpx solid rgba(238,238,238,1);
	margin-top: 20rpx;
	height: 70rpx;
	line-height: 70rpx;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
	padding: 0 20rpx;
	/* text-overflow: ellipsis;
	white-space: nowrap; */
}
.hengxian{
	height: 2rpx;
	width: 300rpx;
	padding-left: 20rpx;
	background-color: #888888;
}
.tab-text {
	font-size: 18rpx;
	font-family: SourceHanSansCN-Regular, SourceHanSansCN;
	font-weight: 400;
	color: rgba(51, 51, 51, 1);
}
.wait-deal-view {
  height: 72rpx;
  line-height: 72rpx;
  background: white;
  -webkit-box-shadow: 0rpx 1rpx 4rpx 0rpx rgba(187, 187, 187, 0.5);
          box-shadow: 0rpx 1rpx 4rpx 0rpx rgba(187, 187, 187, 0.5);
  border-radius: 6rpx;
  border: 1rpx solid #eeeeee;
  font-size: 28rpx;
  color: #333;
  margin-top: 20rpx;
}
.wait-deal-view .wait-deal-text {
  padding: 0 20rpx;
}

