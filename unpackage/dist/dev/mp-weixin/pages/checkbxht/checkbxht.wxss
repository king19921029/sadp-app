.bxht-nav-bar {
  padding-bottom: 100rpx;
}
.bxht-view-box {
  width: 90%;
  margin: 5%;
  position: absolute;
  top: 8%;
  right: 0;
  left: 0;
  background: white;
  border-radius: 16px 16px 0px 0px;
  z-index: 999;
}
.bxht-view-box .bxht-key-value-view {
  margin: 40rpx 0 0 30rpx;
  font-size: 26rpx;
  font-family: SourceHanSansCN-Regular,SourceHanSansCN;
  font-weight: 400;
  color: #333333;
}
.bxht-button-view {
  padding-bottom: 100rpx;
}
.bxht-button-view .bxht-button-agreen, .bxht-button-view .bxht-button-unagreen {
  border: none;
  border-radius: 100rpx;
  background-color: #3574FA;
  color: #fff;
  font-size: 28rpx;
  height: 80rpx;
  line-height: 80rpx;
  margin-top: 100rpx;
  width: 90%;
}
.bxht-button-view .bxht-button-unagreen {
  margin-top: 30rpx;
}

