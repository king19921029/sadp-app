
.uni-icon {
	vertical-align: top;
}
.uni-navbar {
	display: block;
	position: relative;
	width: 100%;
	background-color: #FFFFFF;
	/* background: url('/static/imgs/shouye/yewu.png'); */
	/* overflow: hidden; */
}
.uni-navbar view {
	/* line-height: 90upx; */
	/* padding-top: 10px; */
}
.uni-navbar-shadow {
	-webkit-box-shadow: 0 1px 6px #ccc;
	        box-shadow: 0 1px 6px #ccc;
}
.uni-navbar.uni-navbar-fixed {
	position: fixed;
	z-index: 998;
}
.uni-navbar-header {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	        flex-direction: row;
	width: 100%;
	/* line-height: 100upx; */
	font-size: 32rpx;
}
.uni-navbar-header {
	height: auto;
}
.uni-navbar-header .uni-navbar-header-btns {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: end;
	-webkit-align-items: flex-end;
	        align-items: flex-end;
	-webkit-flex-wrap: nowrap;
	        flex-wrap: nowrap;
	-webkit-flex-shrink: 0;
	        flex-shrink: 0;
	width: 120rpx;
	padding: 0 12rpx;
	padding-bottom: 10rpx;
}
.uni-navbar-header .uni-navbar-header-btns:first-child {
	padding-left: 20rpx;
}
.uni-navbar-header .uni-navbar-header-btns:last-child {
	width: 60rpx;
}
.uni-navbar-container {
	width: 100%;
	margin: 0 10rpx;
	position: relative;
}
.uni-navbar-container-title {
	/* height: 120upx; */
