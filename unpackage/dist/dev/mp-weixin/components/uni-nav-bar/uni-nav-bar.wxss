	/* display: flex; */
	/* justify-content: center; */
	font-size: 36rpx;
	/* text-align: center; */
	font-weight: 700;
	padding-right: 60rpx;
	/* align-items: flex-end; */
	position: absolute;
	width: 100%;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
	text-align: center;
	bottom: 0;
}

